<div class="container-fluid">

	<h1>Articles</h1>
	<ul class="list-group">
		{{#each article}}
			<div id="article">
			  	<li class="list-group-item">{{this.title}}
					<button type="submit" style="float: right; position: relative;" id="save" data-attr="{{_id}}" >Save Article</button>
			  	</li>
			  	<li class="list-group-item">{{this.link}}</li>
			  	<!-- <li class="list-group-item">{{this.comment}}</li> -->
			  	<br>
			</div>
		{{/each}}
	</ul>

</div>
