<div class="container-fluid">

	<h1>Articles</h1>
	<ul class="list-group">
		
			{{#each article}}
				<div id="article" data-attr={{this._id}}>
			  	<li class="list-group-item">{{this.title}}</li>
			  	<li class="list-group-item">{{this.link}}</li>
			  <!-- <li class="list-group-item">{{this.comment}}</li> -->
			</div>
			{{/each}}
	</ul>

</div>
